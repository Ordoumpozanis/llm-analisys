generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Chat {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  messages          Json
  globalStatistics  Json 
  sessionInfo       Json 
  userData          Json 
  type              String
  date              DateTime
  uuid              String    
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}


